{
  "name": "topten",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "start:prod": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "node server.js",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "devDependencies": {
    "concurrently": "^3.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whoisjohnkim/topTen.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/whoisjohnkim/topTen/issues"
  },
  "homepage": "https://github.com/whoisjohnkim/topTen#readme",
  "dependencies": {
    "express": "^4.16.4",
    "express-router": "0.0.1",
    "mongoose": "^5.3.4"
  }
}
